<% provide(:title, @user.name) %>
Name: <%= @user.name %> </br>
Email: <%= @user.email %> </br>
Created at: <%= @user.created_at %> </br>
Updated at: <%= @user.updated_at %> </br>
Time now: <%= Time.now %>


<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
    <h1>
      <%= gravatar_for @user %>
      <%= @user.name %>
    </h1>
    </section>
  </aside>
</div>
